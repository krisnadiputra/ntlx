#include<iostream>
#include<cstdio>

using namespace std;

int c,n;

int main()
{
	c=0;
	cin>>n;
	while(n>9)
	{
		n = n/10;
		c++;
	}
	switch(c)
	{
		case 0:
			cout<<"satuan"<<endl;
			break;
		case 1:
			cout<<"puluhan"<<endl;
			break;
		case 2:
			cout<<"ratusan"<<endl;
			break;
		case 3:
			cout<<"ribuan"<<endl;
			break;
		case 4:
			cout<<"puluhribuan"<<endl;
			break;
	}
	return 0;
}